{
  "name": "agenda-rest",
  "version": "1.2.3",
  "description": "Scheduling as a Service",
  "main": "./dist/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "xo --fix",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "test": "ava dist/test.js && npm run lint",
    "start": "npm run dev && node cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/agenda/agenda-rest.git"
  },
  "files": [
    "cli.js",
    "settings.js",
    "dist"
  ],
  "keywords": [
    "schedule",
    "scheduling",
    "scheduler",
    "webservice",
    "microservice",
    "restful",
    "rest",
    "agenda"
  ],
  "author": "Keyvan Mir Mohammad Sadeghi <keyvan.m.sadeghi@gmail.com>",
  "license": "MIT",
  "bin": {
    "agenda-rest": "cli.js"
  },
  "bugs": {
    "url": "https://github.com/agenda/agenda-rest/issues"
  },
  "homepage": "https://github.com/agenda/agenda-rest#README",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "ava": "3.7.0",
    "babel-loader": "8.1.0",
    "supertest": "4.0.2",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "xo": "0.29.1"
  },
  "dependencies": {
    "agenda": "^3.0.0",
    "async-counter": "^1.1.0",
    "babel-runtime": "^6.26.0",
    "commander": "^5.0.0",
    "koa": "^2.10.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.0",
    "pythonic": "^2.0.3",
    "regenerator-runtime": "^0.13.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "engines": {
    "node": ">=8"
  },
  "xo": {
    "space": 2,
    "rules": {
      "ava/no-inline-assertions": 0,
      "import/namespace": 0,
      "promise/param-names": 0,
      "promise/prefer-await-to-then": 0,
      "max-params": [
        "error",
        5
      ],
      "max-nested-callbacks": [
        "error",
        5
      ]
    }
  }
}
